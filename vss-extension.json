{
  "manifestVersion": 1,
  "id": "ReleaseNotesGenerator",
  "name": "Release notes generator",
  "version": "1.0.0",
  "publisher": "IeuanWalker",
  "public": true,
  "CustomerQnASupport": {
    "enableqna":"true",
    "url": "https://github.com/IeuanWalker/AzureDevops-GenerateReleaseNotes/issues"
  },
  "description": "Generate release notes from a range of commits in your repository.",
  "categories": [
    "Azure Pipelines"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Release",
    "Package",
    "Publish"
  ],   
  "branding": {
    "color": "#3D9AE2",
    "theme": "dark"
  },
  "icons": {
    "default": "images/logo.png"
  },
  "links": {
    "home": {
      "uri": "https://github.com/IeuanWalker/AzureDevops-GenerateReleaseNotes"
    },
    "getstarted": {
      "uri": "https://github.com/IeuanWalker/AzureDevops-GenerateReleaseNotes"
    },
    "issues": {
      "uri": "https://github.com/IeuanWalker/AzureDevops-GenerateReleaseNotes/issues"
    }
  },
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "files": [
    {
      "path": "CommitRangeReleaseNotesTask"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/IeuanWalker/AzureDevops-GenerateReleaseNotes"
  },
  "contributions": [
    {
      "id": "commit-range-release-notes-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "CommitRangeReleaseNotesTask"
      }
    }
  ]
}